--// TIMEBOMB DUELS SCRIPT | OBFUSCATED
local a=game:GetService("Players")local b=game:GetService("RunService")local c=game:GetService("StarterGui")local d=a.LocalPlayer local e=d:GetMouse()local f=false local g=false local h=false local i,j,k local l=false local m=0 local n=50 local o=1.2 local p=0.5 local q=4 local r=nil local s=false local t=0 local u=nil local v=nil local w=nil local x=10 local function y(z,A)local B=A-z local C=RaycastParams.new()C.FilterDescendantsInstances={i}C.FilterType=Enum.RaycastFilterType.Exclude C.IgnoreWater=true return workspace:Raycast(z,B,C)end local function D(E)if not j or not E then return false end local F=j.Position+Vector3.new(0,2,0)local G=E.Position+Vector3.new(0,2,0)local H=y(F,G)if H and H.Instance and H.Instance.CanCollide and H.Instance.Material~=Enum.Material.ForceField then return false end return true end local function I(J)if l or not j or tick()-m<p then return end l=true m=tick()local K=j.CFrame local L=tick()local M=2 local N=J.Position local O b.Heartbeat:Connect(function()local P=tick()-L if P>=o then O:Disconnect()if j and j.Parent then j.CFrame=K end l=false return end if J and J.Parent and D(J)then N=J.Position end local Q=(P/o)*math.pi*2*q local R=Vector3.new(math.cos(Q)*M,0,math.sin(Q)*M)j.CFrame=CFrame.new(N+R,N)end)end local function S()if l or not j or tick()-m<p then return end local T=j.CFrame.LookVector local U=j.Position local V,W=nil,math.huge for _,X in a:GetPlayers()do if X~=d and X.Character and X.Character:FindFirstChild("HumanoidRootPart")then local Y=X.Character.HumanoidRootPart local Z=(Y.Position-U).Magnitude if Z<=n and T:Dot((Y.Position-U).Unit)>0.7 and Z<W and D(Y)then W,V=Z,X end end end if V then I(V.Character.HumanoidRootPart)end end local function aa()if l or not j or tick()-m<p then return end local ab,ac=nil,math.huge local ad=j.Position for _,ae in a:GetPlayers()do if ae~=d and ae.Character and ae.Character:FindFirstChild("HumanoidRootPart")then local af=ae.Character.HumanoidRootPart local ag=(af.Position-ad).Magnitude if ag<=n and ag<ac and D(af)then ac,ab=ag,ae end end end if ab then I(ab.Character.HumanoidRootPart)end end local function ah()if not r or not r.Character or not r.Character:FindFirstChild("HumanoidRootPart")then pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="No target selected!",Duration=3})end)return end if l or not j or tick()-m<p then return end local ai=r.Character.HumanoidRootPart if not D(ai)then pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="Target behind wall!",Duration=2})end)return end I(ai)end local function aj()if k:FindFirstChild("Target Selector")then return end local ak=Instance.new("Tool")ak.Name="Target Selector"ak.RequiresHandle=false ak.Parent=k local al=Instance.new("Part",ak)al.Name="Handle"al.Size=Vector3.new(0.1,0.1,0.1)al.Transparency=1 al.CanCollide=false ak.Activated:Connect(function()local am=e.Target if not am then return end local an=am.Parent if not an then return end local ao=an:FindFirstChild("Humanoid")if not ao then return end local ap=a:GetPlayerFromCharacter(an)if not ap or ap==d then return end if r==ap then return end r=ap pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="Targeted: @"..ap.DisplayName,Duration=3})end)end)pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="Target Tool Given!",Duration=3})end)end local function aq()if k:FindFirstChild("Teleport Tool")then return end local ar=Instance.new("Tool")ar.Name="Teleport Tool"ar.RequiresHandle=false ar.Parent=k local as=Instance.new("Part",ar)as.Name="Handle"as.Size=Vector3.new(0.1,0.1,0.1)as.Transparency=1 as.CanCollide=false ar.Activated:Connect(function()local at=e.Target if not at then return end local au=at.Position+Vector3.new(0,5,0)local av=workspace:Raycast(au,Vector3.new(0,-100,0),RaycastParams.new())if av then au=av.Position+Vector3.new(0,3,0)end if j and j.Parent then j.CFrame=CFrame.new(au)end end)pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="TP Tool Given!",Duration=3})end)end local function aw()if r then pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="No longer targeting @"..r.DisplayName,Duration=3})end)end r=nil local ax=k:FindFirstChild("Target Selector")if ax then ax:Destroy()end f=false end local function ay()local az=k:FindFirstChild("Teleport Tool")if az then az:Destroy()end g=false pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="TP Tool Removed!",Duration=3})end)end local function aA()local aB,aC=nil,math.huge if i and j then for _,aD in a:GetPlayers()do if aD~=d and aD.Character and aD.Character:FindFirstChild("HumanoidRootPart")then local aE=(aD.Character.HumanoidRootPart.Position-j.Position).Magnitude if aE<aC then aC,aB=aE,aD end end end end return aB end local function aF(aG)if w then w:Destroy()end local aH=aG:FindFirstChild("HumanoidRootPart")if aH then local aI=Instance.new("BillboardGui",aH)aI.Name="TargetMarker"aI.Size=UDim2.new(0,8,0,8)aI.AlwaysOnTop=true aI.StudsOffset=Vector3.new(0,3,0)local aJ=Instance.new("Frame",aI)aJ.Size=UDim2.new(1,0,1,0)aJ.BackgroundColor3=Color3.fromRGB(0,255,0)aJ.BorderSizePixel=0 local aK=Instance.new("UICorner",aJ)aK.CornerRadius=UDim.new(1,0)w=aI end end local function aL(aM)s=not s if s then aM.Text="Follow: ON" aM.BackgroundColor3=Color3.fromRGB(0,200,0)t=tick()+x u=aA()if u and u.Character then aF(u.Character)end else aM.Text="Follow: OFF" aM.BackgroundColor3=Color3.fromRGB(180,0,0)t=0 u=nil if w then w:Destroy()w=nil end end end local function aN(aO)i=aO j=aO:WaitForChild("HumanoidRootPart",10)k=d:WaitForChild("Backpack")if f then aj()end if g then aq()end aO.ChildAdded:Connect(function(aP)if aP:IsA("Tool")and s then t=tick()+x u=aA()if u and u.Character then aF(u.Character)end end end)aO.ChildRemoved:Connect(function(aQ)if aQ:IsA("Tool")then local aR=false for _,aS in aO:GetChildren()do if aS:IsA("Tool")then aR=true break end end if not aR then t=0 u=nil if w then w:Destroy()w=nil end end end end)end local function aT()if h then return end h=true local aU=Instance.new("ScreenGui")aU.Name="TimebombGUI"aU.ResetOnSpawn=false aU.Parent=d:WaitForChild("PlayerGui")local aV=Instance.new("Frame",aU)aV.Size=UDim2.new(0,250,0,250)aV.Position=UDim2.new(0.5,-125,0.5,-125)aV.BackgroundColor3=Color3.fromRGB(25,25,35)aV.BorderSizePixel=2 aV.BorderColor3=Color3.fromRGB(255,50,50)aV.Active=true aV.Draggable=true local aW=Instance.new("Frame",aV)aW.Size=UDim2.new(1,0,0,30)aW.BackgroundColor3=Color3.fromRGB(40,40,55)local aX=Instance.new("TextLabel",aW)aX.Size=UDim2.new(1,-130,1,0)aX.Position=UDim2.new(0,5,0,0)aX.BackgroundTransparency=1 aX.Text="Timebomb | slivkineepy"aX.TextColor3=Color3.fromRGB(255,255,100)aX.Font=Enum.Font.GothamBold aX.TextSize=14 aX.TextXAlignment=Enum.TextXAlignment.Left local aY=Instance.new("TextButton",aW)aY.Size=UDim2.new(0,40,0,40)aY.Position=UDim2.new(1,-85,0,-5)aY.BackgroundTransparency=1 aY.Text="-"aY.TextColor3=Color3.fromRGB(200,200,200)aY.Font=Enum.Font.GothamBold aY.TextSize=20 local aZ=Instance.new("TextButton",aW)aZ.Size=UDim2.new(0,40,0,40)aZ.Position=UDim2.new(1,-85,0,-5)aZ.BackgroundTransparency=1 aZ.Text="+"aZ.TextColor3=Color3.fromRGB(100,255,100)aZ.Font=Enum.Font.GothamBold aZ.TextSize=20 aZ.Visible=false local ba=Instance.new("TextButton",aW)ba.Size=UDim2.new(0,40,0,40)ba.Position=UDim2.new(1,-45,0,-5)ba.BackgroundTransparency=1 ba.Text="X"ba.TextColor3=Color3.fromRGB(255,80,80)ba.Font=Enum.Font.GothamBold ba.TextSize=20 local bb=Instance.new("Frame",aV)bb.Size=UDim2.new(1,0,1,-30)bb.Position=UDim2.new(0,0,0,30)bb.BackgroundTransparency=1 bb.ClipsDescendants=true local bc=Instance.new("ScrollingFrame",bb)bc.Size=UDim2.new(1,-20,1,0)bc.Position=UDim2.new(0,10,0,0)bc.BackgroundTransparency=1 bc.ScrollBarThickness=6 bc.AutomaticCanvasSize=Enum.AutomaticSize.Y bc.CanvasSize=UDim2.new(0,0,0,0)bc.ScrollingDirection=Enum.ScrollingDirection.Y bc.TopImage="rbxasset://textures/ui/Scroll/scroll-middle.png"bc.BottomImage="rbxasset://textures/ui/Scroll/scroll-middle.png"bc.CanvasPosition=Vector2.new(0,0)local bd=Instance.new("UIListLayout",bc)bd.Padding=UDim.new(0,6)bd.FillDirection=Enum.FillDirection.Vertical bd.SortOrder=Enum.SortOrder.LayoutOrder local be=aV.Size local bf=UDim2.new(0,250,0,30)local bg=false aY.MouseButton1Click:Connect(function()if bg then return end bg=true aY.Visible=false aZ.Visible=true bb.Visible=false aV:TweenSize(bf,"Out","Quad",0.3,true)end)aZ.MouseButton1Click:Connect(function()if not bg then return end bg=false aZ.Visible=false aY.Visible=true aV:TweenSize(be,"Out","Quad",0.3,true,function()bb.Visible=true end)end)ba.MouseButton1Click:Connect(function()local bh=Instance.new("ScreenGui")bh.Parent=d.PlayerGui local bi=Instance.new("Frame",bh)bi.Size=UDim2.new(0,220,0,110)bi.Position=UDim2.new(0.5,-110,0.5,-55)bi.BackgroundColor3=Color3.fromRGB(30,30,40)bi.BorderSizePixel=2 bi.BorderColor3=Color3.fromRGB(255,50,50)local bj=Instance.new("TextLabel",bi)bj.Size=UDim2.new(1,0,0.5,0)bj.BackgroundTransparency=1 bj.Text="Close GUI?"bj.TextColor3=Color3.fromRGB(255,255,100)bj.Font=Enum.Font.GothamBold bj.TextSize=18 local bk=Instance.new("TextButton",bi)bk.Size=UDim2.new(0.45,0,0.35,0)bk.Position=UDim2.new(0.05,0,0.55,0)bk.BackgroundColor3=Color3.fromRGB(50,150,50)bk.Text="Yes"bk.TextColor3=Color3.new(1,1,1)bk.Font=Enum.Font.GothamBold bk.TextSize=16 local bl=Instance.new("TextButton",bi)bl.Size=UDim2.new(0.45,0,0.35,0)bl.Position=UDim2.new(0.5,0,0.55,0)bl.BackgroundColor3=Color3.fromRGB(150,50,50)bl.Text="No"bl.TextColor3=Color3.new(1,1,1)bl.Font=Enum.Font.GothamBold bl.TextSize=16 bk.MouseButton1Click:Connect(function()aw()ay()aU:Destroy()bh:Destroy()h=false end)bl.MouseButton1Click:Connect(function()bh:Destroy()end)end)local bm={}local function bn(bo,bp,bq)local br=Instance.new("TextButton",bc)br.Size=UDim2.new(1,0,0,34)br.BackgroundColor3=bp br.Text=bo br.TextColor3=Color3.fromRGB(255,255,255)br.Font=Enum.Font.Gotham br.TextSize=13 br.AutoButtonColor=false local bs,bt=bo,bp br.MouseButton1Click:Connect(function()local bu=tick()if bm[br]and bu-bm[br]<0.5 then return end bm[br]=bu br.Text="Success!"br.BackgroundColor3=Color3.fromRGB(0,170,0)br.TextColor3=Color3.new(1,1,1)spawn(bq)task.delay(0.5,function()if br and br.Parent then br.Text=bs br.BackgroundColor3=bt br.TextColor3=Color3.fromRGB(255,255,255)end end)end)br.MouseEnter:Connect(function()if br.Text~="Success!"then br.BackgroundColor3=bp:lerp(Color3.new(1,1,1),0.2)end end)br.MouseLeave:Connect(function()if br.Text~="Success!"then br.BackgroundColor3=bp end end)end bn("Orbit by Look",Color3.fromRGB(55,75,65),S)bn("Orbit Nearest",Color3.fromRGB(60,70,90),aa)bn("Give Target Tool",Color3.fromRGB(0,140,220),function()f=true aj()end)bn("Orbit Target",Color3.fromRGB(90,60,110),ah)bn("Remove Target",Color3.fromRGB(160,60,60),aw)bn("Give TP Tool",Color3.fromRGB(0,170,220),function()g=true aq()end)bn("Clear TP Tool",Color3.fromRGB(220,100,0),ay)local bv=Instance.new("TextButton",bc)bv.Size=UDim2.new(1,0,0,34)bv.BackgroundColor3=Color3.fromRGB(180,0,0)bv.Text="Follow: OFF"bv.TextColor3=Color3.fromRGB(255,255,255)bv.Font=Enum.Font.Gotham bv.TextSize=13 bv.AutoButtonColor=false bv.MouseButton1Click:Connect(function()aL(bv)end)bv.MouseEnter:Connect(function()if bv.Text~="Success!"then bv.BackgroundColor3=bv.BackgroundColor3:lerp(Color3.new(1,1,1),0.2)end end)bv.MouseLeave:Connect(function()if bv.Text~="Success!"then bv.BackgroundColor3=(s and Color3.fromRGB(0,200,0)or Color3.fromRGB(180,0,0))end end)end b.RenderStepped:Connect(function()if s and tick()<t then if not u or not u.Character or not u.Character:FindFirstChild("HumanoidRootPart")or not u.Character:FindFirstChild("Humanoid")or u.Character.Humanoid.Health<=0 then u=aA()if u and u.Character then aF(u.Character)else if w then w:Destroy()w=nil end return end end if u and u.Character and i then local bw=u.Character:FindFirstChild("HumanoidRootPart")local bx=i:FindFirstChild("Humanoid")local by=i:FindFirstChild("HumanoidRootPart")if bw and bx and by then bx:MoveTo(bw.Position)end end end end)spawn(function()pcall(function()c:SetCore("SendNotification",{Title="Timebomb duels script",Text="Executed | by slivkineepy",Duration=5})end)end)d.CharacterAdded:Connect(aN)if d.Character then aN(d.Character)end aT()
